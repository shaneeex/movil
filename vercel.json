{
  "version": 2,
  "functions": {
    "api/index.js": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    { "source": "/api", "destination": "/api/index.js" },
    { "source": "/api/(.*)", "destination": "/api/index.js" },
    { "source": "/admin", "destination": "/api/index.js" },
    { "source": "/admin/(.*)", "destination": "/api/index.js" },
    { "source": "/p/(.*)", "destination": "/api/index.js" }
  ]
}

