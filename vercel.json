{
  "version": 2,
  "functions": {
    "api/[...route].js": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    { "source": "/admin", "destination": "/api/[...route].js" },
    { "source": "/admin.html", "destination": "/api/[...route].js" },
    { "source": "/admin/(.*)", "destination": "/api/[...route].js" },
    { "source": "/p/(.*)", "destination": "/api/[...route].js" }
  ]
}

