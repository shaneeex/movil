{
  "version": 2,
  "functions": {
    "api/server.js": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/api",
      "destination": "/api/server"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/server"
    },
    {
      "source": "/p/(.*)",
      "destination": "/api/server"
    }
  ]
}
